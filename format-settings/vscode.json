{
    "workbench.iconTheme": "helium-icon-theme",
    "workbench.colorTheme": "Default Dark+",
    "window.zoomLevel": -0.4,
    "terminal.integrated.fontSize": 17,
    "html.format.wrapLineLength": 120,
    "github.copilot.editor.enableAutoCompletions": true,
    "editor.fontFamily": "Consolas, Cascadia Code, Source Code Pro, Monospace",
    "editor.quickSuggestions": { "strings": "on" },
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent",
    "editor.linkedEditing": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            { "scope": "variable", "settings": { "foreground": "#9cdcf5" } },
            { "scope": "variable.language", "settings": { "foreground": "#efb484" } },
            { "scope": "variable.name", "settings": { "foreground": "#dddddd" } },
            { "scope": "variable.parameter", "settings": { "foreground": "#efb484" } },
