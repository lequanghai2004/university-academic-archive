            { "scope": "entity.name.type", "settings": { "foreground": "#7df6ab" } },
            { "scope": "entity.other", "settings": { "foreground": "#dddddd" } },
            { "scope": "entity.other.attribute-name", "settings": { "foreground": "#9cdcf5" } },
            { "scope": "entity.other.inherited-class", "settings": { "foreground": "#66ffa0" } },
            { "scope": "constant", "settings": { "foreground": "#dddddd" } },
            // { "scope": "constant.character", "settings": { "foreground": "#000" } },
            // { "scope": "constant.character.escape", "settings": { "foreground": "#000" } },
            // { "scope": "constant.numeric", "settings": { "foreground": "#000" } },
            // { "scope": "constant.numeric.float", "settings": { "foreground": "#000" } },
            // { "scope": "constant.numeric.hex", "settings": { "foreground": "#000" } },
            // { "scope": "constant.numeric.integer", "settings": { "foreground": "#000" } },
            // { "scope": "constant.numeric.octal", "settings": { "foreground": "#000" } },
            // { "scope": "constant.other", "settings": { "foreground": "#000" } },
            { "scope": "constant.regexp", "settings": { "foreground": "#ff3838" } },
            // { "scope": "constant.rgb-value", "settings": { "foreground": "#000" } },
            // { "scope": "meta", "settings": { "foreground": "#000" } },
            { "scope": "meta.block", "settings": { "foreground": "#dddddd" } },
            { "scope": "meta.cast", "settings": { "foreground": "#dddddd" } },
            { "scope": "meta.class", "settings": { "foreground": "#dddddd" } },
            { "scope": "meta.function", "settings": { "foreground": "#ff3838" } },
            { "scope": "meta.function-call", "settings": { "foreground": "#f9f8ab" } },
            { "scope": "meta.preprocessor", "settings": { "foreground": "#dddddd" } },
            { "scope": "meta.return-type", "settings": { "foreground": "#dddddd" } },
            { "scope": "meta.selector", "settings": { "foreground": "#dddddd" } },
            { "scope": "meta.tag", "settings": { "foreground": "#dddddd" } },
