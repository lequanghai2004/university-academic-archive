            { "scope": "variable.other.readwrite", "settings": { "foreground": "#9cdcf5" } },
            { "scope": "support", "settings": { "foreground": "#9cdcf5" } },
            { "scope": "support.class", "settings": { "foreground": "#f9f8ab" } },
            { "scope": "support.constant", "settings": { "foreground": "#9cdcf5" } },
            { "scope": "support.function", "settings": { "foreground": "#f9f8ab" } },
            { "scope": "support.other", "settings": { "foreground": "#295b84" } },
            { "scope": "support.type", "settings": { "foreground": "#7df6ab" } },
            { "scope": "support.type.property-name", "settings": { "foreground": "#9cdcf5" } },
            { "scope": "support.variable", "settings": { "foreground": "#dddddd" } },
            { "scope": "keyword", "settings": { "foreground": "#dddddd" } },
