            { "scope": "keyword.operator.new", "settings": { "foreground": "#f37fda" } },
            { "scope": "keyword.other", "settings": { "foreground": "#f37fda" } },
            { "scope": "entity", "settings": { "foreground": "#dddddd" } },
            { "scope": "entity.name", "settings": { "foreground": "#7df6ab" } },
